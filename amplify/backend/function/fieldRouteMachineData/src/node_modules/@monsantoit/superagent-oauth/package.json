{
  "name": "@monsantoit/superagent-oauth",
  "version": "1.3.1",
  "description": "super agent with additional capabilities for bayer oauth authentication",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "postversion": "git push origin master && git push origin --tags && npm publish --registry=https://npm.platforms.engineering --scope=@monsantoit .",
    "test": "npm run test:only",
    "test:only": "__DEV_TEAM=pipeline-dev mocha test --recursive --timeout 30000",
    "pretest": "npm run format && npm run lint",
    "format": "prettier --config .prettierrc.json --write './**/@(*.js|*.json)'",
    "lint": "eslint . --ext .js",
    "prepare": "rm -f .env.json && npm prune --production"
  },
  "repository": {
    "type": "git",
    "url": "git@github.platforms.engineering:PD-LAB/superagent-oauth.git"
  },
  "author": "Stephen Grodeon <srgrod@monsanto.com> (https://github.com/srgrodeon)",
  "license": "ISC",
  "devDependencies": {
    "@monsantoit/eslint-config-street": "~4.0.0",
    "babel-eslint": "~10.1.0",
    "chai": "~4.2.0",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-import": "~2.20.2",
    "eslint-plugin-no-only-tests": "~2.4.0",
    "eslint-plugin-prettier": "~3.1.3",
    "husky": "~4.2.5",
    "mocha": "~6.2.2",
    "nock": "~11.7.0",
    "prettier": "~2.0.5",
    "pretty-quick": "~2.0.1",
    "random-string": "~0.2.0",
    "sinon": "^9.0.2"
  },
  "dependencies": {
    "@monsantoit/oauth-azure": "~1.1.0",
    "@monsantoit/oauth-ping": "~4.6.1",
    "@monsantoit/vault-cf-services": "^1.3.0",
    "async": "~3.1.0",
    "lodash": "^4.17.20",
    "node-vault": "^0.9.15",
    "superagent": "~5.1.0",
    "vault-auth-aws": "~0.1.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "post-commit": "git update-index -g",
      "pre-push": "npm run test"
    }
  }
}
